    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "framer-motion": "^12.23.24",
    "fs-extra": "^11.3.2",
    "gsap": "^3.13.0",
    "immer": "^11.0.0",
    "ipaddr.js": "^2.2.0",
    "is-ci": "^4.1.0",
    "is-docker": "^4.0.0",
    "is-localhost-ip": "^2.0.0",
    "isbot": "^5.1.32",
    "jose": "^6.1.2",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "kafkajs": "^2.2.4",
    "lenis": "^1.3.15",
    "lucide-react": "^0.554.0",
    "maxmind": "^5.0.1",
    "md5": "^2.3.0",
    "next": "16.0.4",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "pure-rand": "^7.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
