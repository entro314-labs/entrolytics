generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
  binaryTargets = ["native", "linux-musl", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model event_data {
  event_data_id    String    @id @db.Uuid
  website_id       String    @db.Uuid
  website_event_id String    @db.Uuid
  data_key         String    @db.VarChar(500)
  string_value     String?   @db.VarChar(500)
  number_value     Decimal?  @db.Decimal(19, 4)
  date_value       DateTime? @db.Timestamptz(6)
  data_type        Int
  created_at       DateTime? @default(now()) @db.Timestamptz(6)

  @@index([created_at])
  @@index([website_event_id])
  @@index([website_id, created_at, data_key])
  @@index([website_id, created_at])
  @@index([website_id])
}

model link {
  link_id    String    @id @unique @db.Uuid
  name       String    @db.VarChar(100)
  url        String    @db.VarChar(500)
  slug       String    @unique @db.VarChar(100)
  user_id    String?   @db.Uuid
  org_id     String?   @db.Uuid
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)
  deleted_at DateTime? @db.Timestamptz(6)

  @@index([created_at])
  @@index([org_id])
  @@index([slug])
  @@index([user_id])
}

model org {
  org_id      String    @id @unique @db.Uuid
  name        String    @db.VarChar(50)
  access_code String?   @unique @db.VarChar(50)
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  updated_at  DateTime? @db.Timestamptz(6)
  deleted_at  DateTime? @db.Timestamptz(6)
  logo_url    String?   @db.VarChar(2183)

  @@index([access_code])
}

model org_user {
  org_user_id String    @id @unique @db.Uuid
  org_id      String    @db.Uuid
  user_id     String    @db.Uuid
  role        String    @db.VarChar(50)
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  updated_at  DateTime? @db.Timestamptz(6)

  @@index([org_id])
  @@index([user_id])
}

model pixel {
  pixel_id   String    @id @unique @db.Uuid
  name       String    @db.VarChar(100)
  slug       String    @unique @db.VarChar(100)
  user_id    String?   @db.Uuid
  org_id     String?   @db.Uuid
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)
  deleted_at DateTime? @db.Timestamptz(6)

  @@index([created_at])
  @@index([org_id])
  @@index([slug])
  @@index([user_id])
}

model report {
  report_id   String    @id @unique @db.Uuid
  user_id     String    @db.Uuid
  website_id  String    @db.Uuid
  type        String    @db.VarChar(50)
  name        String    @db.VarChar(200)
  description String    @db.VarChar(500)
  parameters  Json
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  updated_at  DateTime? @db.Timestamptz(6)

  @@index([name])
  @@index([type])
  @@index([user_id])
  @@index([website_id])
}

model revenue {
  revenue_id String    @id @unique @db.Uuid
  website_id String    @db.Uuid
  session_id String    @db.Uuid
  event_id   String    @db.Uuid
  event_name String    @db.VarChar(50)
  currency   String    @db.VarChar(10)
  revenue    Decimal?  @db.Decimal(19, 4)
  created_at DateTime? @default(now()) @db.Timestamptz(6)

  @@index([session_id])
  @@index([website_id, created_at])
  @@index([website_id])
  @@index([website_id, session_id, created_at])
}

model segment {
  segment_id String    @id @unique @db.Uuid
  website_id String    @db.Uuid
  type       String    @db.VarChar(50)
  name       String    @db.VarChar(200)
  parameters Json
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)

  @@index([website_id])
}

model session {
  session_id  String    @id @unique @db.Uuid
  website_id  String    @db.Uuid
  browser     String?   @db.VarChar(20)
  os          String?   @db.VarChar(20)
  device      String?   @db.VarChar(20)
  screen      String?   @db.VarChar(11)
  language    String?   @db.VarChar(35)
  country     String?   @db.Char(2)
  region      String?   @db.VarChar(20)
  city        String?   @db.VarChar(50)
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  distinct_id String?   @db.VarChar(50)

  @@index([created_at])
  @@index([website_id, created_at, browser])
  @@index([website_id, created_at, city])
  @@index([website_id, created_at, country])
  @@index([website_id, created_at, device])
  @@index([website_id, created_at])
  @@index([website_id, created_at, language])
  @@index([website_id, created_at, os])
  @@index([website_id, created_at, region])
  @@index([website_id, created_at, screen])
  @@index([website_id])
}

model session_data {
  session_data_id String    @id @db.Uuid
  website_id      String    @db.Uuid
  session_id      String    @db.Uuid
  data_key        String    @db.VarChar(500)
  string_value    String?   @db.VarChar(500)
  number_value    Decimal?  @db.Decimal(19, 4)
  date_value      DateTime? @db.Timestamptz(6)
  data_type       Int
  created_at      DateTime? @default(now()) @db.Timestamptz(6)
  distinct_id     String?   @db.VarChar(50)

  @@index([created_at])
  @@index([session_id, created_at])
  @@index([session_id])
  @@index([website_id, created_at, data_key])
  @@index([website_id])
}

model user {
  user_id      String    @id @unique @db.Uuid
  role         String    @default("user") @db.VarChar(50)
  created_at   DateTime? @default(now()) @db.Timestamptz(6)
  updated_at   DateTime? @db.Timestamptz(6)
  deleted_at   DateTime? @db.Timestamptz(6)
  display_name String?   @db.VarChar(255)
  clerk_id     String    @unique @db.VarChar(255)
  email        String    @db.VarChar(255)
  first_name   String?   @db.VarChar(255)
  last_name    String?   @db.VarChar(255)
  image_url    String?   @db.VarChar(2183)

  @@index([email])
}

model website {
  website_id String    @id @unique @db.Uuid
  name       String    @db.VarChar(100)
  domain     String?   @db.VarChar(500)
  share_id   String?   @unique @db.VarChar(50)
  reset_at   DateTime? @db.Timestamptz(6)
  user_id    String?   @db.Uuid
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)
  deleted_at DateTime? @db.Timestamptz(6)
  created_by String?   @db.Uuid
  org_id     String?   @db.Uuid

  @@index([created_at])
  @@index([created_by])
  @@index([org_id])
  @@index([share_id])
  @@index([user_id])
}

model website_event {
  event_id        String    @id @db.Uuid
  website_id      String    @db.Uuid
  session_id      String    @db.Uuid
  created_at      DateTime? @default(now()) @db.Timestamptz(6)
  url_path        String    @db.VarChar(500)
  url_query       String?   @db.VarChar(500)
  referrer_path   String?   @db.VarChar(500)
  referrer_query  String?   @db.VarChar(500)
  referrer_domain String?   @db.VarChar(500)
  page_title      String?   @db.VarChar(500)
  event_type      Int       @default(1)
  event_name      String?   @db.VarChar(50)
  visit_id        String    @db.Uuid
  tag             String?   @db.VarChar(50)
  fbclid          String?   @db.VarChar(255)
  gclid           String?   @db.VarChar(255)
  li_fat_id       String?   @db.VarChar(255)
  msclkid         String?   @db.VarChar(255)
  ttclid          String?   @db.VarChar(255)
  twclid          String?   @db.VarChar(255)
  utm_campaign    String?   @db.VarChar(255)
  utm_content     String?   @db.VarChar(255)
  utm_medium      String?   @db.VarChar(255)
  utm_source      String?   @db.VarChar(255)
  utm_term        String?   @db.VarChar(255)
  hostname        String?   @db.VarChar(100)

  @@index([created_at])
  @@index([session_id])
  @@index([visit_id])
  @@index([website_id, created_at, event_name])
  @@index([website_id, created_at, hostname])
  @@index([website_id, created_at])
  @@index([website_id, created_at, page_title])
  @@index([website_id, created_at, referrer_domain])
  @@index([website_id, created_at, tag])
  @@index([website_id, created_at, url_path])
  @@index([website_id, created_at, url_query])
  @@index([website_id])
  @@index([website_id, session_id, created_at])
  @@index([website_id, visit_id, created_at])
}
